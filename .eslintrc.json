{
  "root": true,
  "extends": ["eslint:recommended", "plugin:jsdoc/recommended", "plugin:jest/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["import", "jsdoc", "jest", "prettier"],
  "rules": {
    "no-unused-vars": "warn"
  }
}
